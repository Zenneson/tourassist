{
	"$schema": "https://turbo.build/schema.json",
	"globalDotEnv": [
		".env.local"
	],
	"globalDependencies": [
		".env.local"
	],
	"pipeline": {
		"build": {
			"outputs": [
				".next/**",
				"!.next/cache/**"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"type-check": {}
	}
}