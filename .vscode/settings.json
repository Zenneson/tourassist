{
	"editor.formatOnSave": true,
	"editor.renderWhitespace": "none",
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"editor.tabSize": 2,
	"editor.lightbulb.enabled": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit"
	},
	// "editor.wordWrapColumn": 70,
	"git.terminalAuthentication": false,
	"git.suggestSmartCommit": false,
	"git.autoRepositoryDetection": false,
	"javascript.format.semicolons": "insert",
	"editor.wordWrap": "on",
	"diffEditor.wordWrap": "on",
	"workbench.editor.wrapTabs": true,
	"html.format.wrapLineLength": 140,
	"workbench.startupEditor": "none",
	"editor.fontFamily": "'Fira Code', monospace",
	"liveServer.settings.donotShowInfoMsg": true,
	"editor.fontSize": 13.5,
	//JavaScript formatting
	"javascript.format.enable": true,
	"javascript.format.insertSpaceAfterCommaDelimiter": true,
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
	"javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"javascript.format.insertSpaceAfterSemicolonInForStatements": true,
	"javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
	//"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
	"javascript.format.placeOpenBraceOnNewLineForFunctions": false,
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.inlineSuggest.enabled": true,
	"editor.largeFileOptimizations": false,
	"css.lint.unknownAtRules": "ignore",
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"editor.inlineSuggest.suppressSuggestions": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"github.copilot.advanced": {},
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false
	},
	"inlineChat.mode": "live",
	"explorer.confirmDragAndDrop": false,
	"remote.SSH.remotePlatform": {
		"sandbox.grw9kg.csb": "linux"
	},
	"css.format.spaceAroundSelectorSeparator": true,
	"css.format.maxPreserveNewLines": 1,
	"cssVariables.lookupFiles": [
		"**/*.css",
		"**/*.scss",
		"**/*.sass",
		"**/*.less",
		"node_modules/@mantine/core/styles.css"
	],
	"gitlens.views.commitDetails.files.layout": "tree",
	"gitlens.advanced.messages": {
		"suppressCommitNotFoundWarning": true
	},
	"workbench.iconTheme": "ayu",
	"workbench.colorTheme": "Sapphire (Dim)",
	// "workbench.colorCustomizations": {},
	"color-manager.styles": {
		"selectionBorder": "black",
		"background": "black",
		"buttonBackground": "#000",
		"inputForeground": "#fff", // input-text
		"inputBackground": "#000", // dropdown input on top and inputs in Palette Manager
		"inputBorder": "white"
	},
	"window.zoomLevel": -1,
	"highlight-matching-tag.styles": {
		"opening": {
			"full": {
				"highlight": "rgba(255, 255, 255, 0.1)",
				"outline": "1px solid rgba(255, 255, 255, 0.5)"
			}
		}
	},
	"indentRainbow.indicatorStyle": "light",
	"workbench.settings.applyToAllProfiles": [
		"javascriptBooster.codemodDir"
	],
	"grunt.autoDetect": "on",
	"gulp.autoDetect": "on",
	"task.allowAutomaticTasks": "on",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"diffEditor.maxComputationTime": 0,
	"gitlens.graph.layout": "editor",
	"cloudcode.updateChannel": "Insiders",
	"cloudcode.duetAI.project": "tourassist-836db",
	"cloudcode.duetAI.languages": [
		"javascript",
		"css",
		"json",
		"html"
	],
	"cloudcode.project": "tourassist-836db",
	"cloudcode.autoDependencies": "on",
	"gitProjectManager.baseProjectsFolders": [
		"/workspaces/javascript-node/tourassist"
	],
	"spellright.language": [
		"en"
	],
	"spellright.documentTypes": [
		"markdown",
		"latex",
		"plaintext",
		"javascript"
	]
}