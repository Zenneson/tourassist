{
	"editor.formatOnSave": true,
	"editor.renderWhitespace": "none",
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"editor.tabSize": 2,
	"editor.lightbulb.enabled": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports": "explicit"
	},
	// "editor.wordWrapColumn": 70,
	"git.terminalAuthentication": false,
	"git.suggestSmartCommit": false,
	"git.autoRepositoryDetection": false,
	"javascript.format.semicolons": "insert",
	"editor.wordWrap": "on",
	"diffEditor.wordWrap": "on",
	"workbench.editor.wrapTabs": true,
	"html.format.wrapLineLength": 140,
	"workbench.startupEditor": "none",
	"editor.fontFamily": "'Fira Code', monospace",
	"liveServer.settings.donotShowInfoMsg": true,
	"editor.fontSize": 13.5,
	//JavaScript formatting
	"javascript.format.enable": true,
	"javascript.format.insertSpaceAfterCommaDelimiter": true,
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
	"javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"javascript.format.insertSpaceAfterSemicolonInForStatements": true,
	"javascript.format.insertSpaceBeforeAndAfterBinaryOperators": true,
	//"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.format.placeOpenBraceOnNewLineForControlBlocks": false,
	"javascript.format.placeOpenBraceOnNewLineForFunctions": false,
	"liveServer.settings.donotVerifyTags": true,
	"remote.SSH.enableRemoteCommand": true,
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"explorer.confirmDelete": false,
	"editor.inlineSuggest.enabled": true,
	"git.confirmSync": false,
	"editor.largeFileOptimizations": false,
	"gitlens.gitCommands.skipConfirmations": [
		"fetch:command",
		"stash-push:command",
		"push:command"
	],
	"css.lint.unknownAtRules": "ignore",
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"editor.inlineSuggest.suppressSuggestions": true,
	"cody.experimental.chatPredictions": true,
	"cody.experimental.chatPanel": true,
	"cody.experimental.commandLenses": true,
	"cody.codebase": "https://github.com/Zenneson/tourassist.git",
	"cody.debug.verbose": true,
	"cody.inlineChat.enabled": false,
	"cody.autocomplete.experimental.syntacticPostProcessing": false,
	"cody.autocomplete.completeSuggestWidgetSelection": false,
	"cody.autocomplete.enabled": false,
	"cody.experimental.newSearch": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"github.copilot.advanced": {},
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false
	},
	"inlineChat.mode": "live",
	"explorer.confirmDragAndDrop": false,
	"remote.SSH.remotePlatform": {
		"sandbox.grw9kg.csb": "linux"
	},
	"css.format.spaceAroundSelectorSeparator": true,
	"css.format.maxPreserveNewLines": 1,
	"cssVariables.lookupFiles": [
		"**/*.css",
		"**/*.scss",
		"**/*.sass",
		"**/*.less",
		"node_modules/@mantine/core/styles.css"
	],
	"gitlens.views.commitDetails.files.layout": "tree",
	"gitlens.advanced.messages": {
		"suppressCommitNotFoundWarning": true
	},
	"workbench.iconTheme": "ayu",
	"workbench.colorTheme": "Sapphire (Dim)",
	"color-manager.styles": {
		"selectionBorder": "black",
		"background": "black",
		"buttonBackground": "#000",
		"inputForeground": "#fff", // input-text
		"inputBackground": "#000", // dropdown input on top and inputs in Palette Manager
		"inputBorder": "white"
	},
	"window.zoomLevel": -1,
	"highlight-matching-tag.styles": {
		"opening": {
			"full": {
				"highlight": "rgba(255, 255, 255, 0.1)",
				"outline": "1px solid rgba(255, 255, 255, 0.5)"
			}
		}
	},
	"indentRainbow.indicatorStyle": "light",
	"cSpell.diagnosticLevel": "Warning",
